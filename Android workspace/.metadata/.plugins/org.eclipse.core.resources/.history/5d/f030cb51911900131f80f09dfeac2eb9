package com.markbuikema.juliana32.model;

import android.graphics.Bitmap;

public class Like {

	private String id;
	private String name;
	private Bitmap image;

	public Like(String id, String name, Bitmap image) {
		this.id = id;
		this.name = name;
		this.image = image;
	}

	public String getName() {
		return name;
	}

	public Bitmap getImage() {
		return image;
	}

	public String getId() {
		return id;
	}
}
