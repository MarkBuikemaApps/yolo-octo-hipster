package com.markbuikema.juliana32.model;

import java.util.ArrayList;

public class Season {

	private int year;
	private ArrayList<Team> teams;
	
	public Season(int year) {
		this.year = year;
		teams = new ArrayList<Team>();
	}

	public int getYear() {
		return year;
	}

	public void addTeam(Team team) {
		teams.add(team);
	}
	
	
}
